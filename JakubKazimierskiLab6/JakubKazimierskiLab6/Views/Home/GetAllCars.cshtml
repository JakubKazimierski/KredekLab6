@model List<CarViewModels>
@{
    ViewData["Title"] = "GetAllCars";
}

<h1>GetAllCars</h1>
<!--Iteration through all elements in list-->
@foreach(var car in Model)
{
<div>
    <h2>@car.Model</h2>
    <h4>Producent: @car.Manufacturer</h4>
    <h4>Cena: @car.Price zł</h4>
    <img src="@Url.Content(car.Photo)" alt="@car.Model"/>
    <hr/>

</div>
}